{
    "private": true,
    "name": "my-domain-package",
    "version": "0.1.0",
    "license": "MIT",
    "engines": {
        "node": ">=16"
    },
    "files": [
        "*"
    ],
    "sideEffects": false,
    "scripts": {
        "dev": "npm run storybook & npm run serve",
        "storybook": "storybook dev -h localhost -p 6006 --disable-telemetry",
        "serve": "PORT=3003 webpack serve --mode=production & npm run sandbox",
        "sandbox": "http-server ./examples/html-host -c-1 -p 3000 -o",
        "lint": "eslint '*/**/*.{ts,tsx}' --max-warnings=0",
        "test": "jest",
        "build": "webpack --mode=production",
        "analyze": "ANALYZE=true webpack --mode=production"
    },
    "dependencies": {
        "@adobe/elsie": "alpha",
        "@adobe/event-bus": "alpha",
        "@adobe/fetch-graphql": "alpha",
        "@adobe/storefront-design": "alpha",
        "preact": "^10.3.2"
    },
    "devDependencies": {
        "dotenv": "^16.0.1",
        "http-server": "14.1.1",
        "jest": "^27.0.1"
    }
}